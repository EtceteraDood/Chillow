.container-fluid
    h3.page-header
        b User Info
    .content-block
        h2.content-title
            span.txt-alignment Profile
        .setting-list
            li.setting-details
                p.setting-label Name
                p.setting-text #{userFullName}
            li.setting-details
                p.setting-label Date of Birth
                p.setting-text 12/31/2000
            li.setting-details
                p.setting-label Gender
                p.setting-text Superposition
            li.setting-details
                p.setting-label Country of Residence
                p.setting-text United States
            li.setting-details
                p.setting-label Time Zone
                p.setting-text (UTC-7:00)America/Los Angeles
    .content-block
        h2.content-title
            span.txt-alignment E-mail Address
        .setting-list
            li.setting-details
                p.setting-label E-mail Address
                p.setting-text #{userEmail}